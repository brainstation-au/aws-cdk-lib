exports[`batch-fargate produces correct cloudformation template 1`] = `
"Parameters:
  VPC:
    Type: String
    Default: vpc-1234
  SubnetIds:
    Type: List<String>
    Default: subnet-123,subnet-234
  BootstrapVersion:
    Type: AWS::SSM::Parameter::Value<String>
    Default: /cdk-bootstrap/hnb659fds/version
    Description: Version of the CDK Bootstrap resources in this environment,
      automatically retrieved from SSM Parameter Store. [cdk:skip]
Resources:
  MyBatchBatchServiceRole36DD76B9:
    Type: AWS::IAM::Role
    Properties:
      AssumeRolePolicyDocument:
        Statement:
          - Action: sts:AssumeRole
            Effect: Allow
            Principal:
              Service: batch.amazonaws.com
        Version: 2012-10-17
      Description: AWS Batch Service Role
      ManagedPolicyArns:
        - arn:aws:iam::aws:policy/service-role/AWSBatchServiceRole
  MyBatchComputeEnvironment4D8A6583:
    Type: AWS::Batch::ComputeEnvironment
    Properties:
      Type: MANAGED
      ComputeResources:
        MaxvCpus: 4
        SecurityGroupIds:
          - sg-1234
        Subnets:
          Ref: SubnetIds
        Type: FARGATE
      ServiceRole:
        Fn::GetAtt:
          - MyBatchBatchServiceRole36DD76B9
          - Arn
      State: ENABLED
  MyBatchComputeEnvironmentSpot385E54C0:
    Type: AWS::Batch::ComputeEnvironment
    Properties:
      Type: MANAGED
      ComputeResources:
        MaxvCpus: 4
        SecurityGroupIds:
          - sg-1234
        Subnets:
          Ref: SubnetIds
        Type: FARGATE_SPOT
      ServiceRole:
        Fn::GetAtt:
          - MyBatchBatchServiceRole36DD76B9
          - Arn
      State: ENABLED
  MyBatchJobQueueC944D50B:
    Type: AWS::Batch::JobQueue
    Properties:
      ComputeEnvironmentOrder:
        - ComputeEnvironment:
            Ref: MyBatchComputeEnvironmentSpot385E54C0
          Order: 1
        - ComputeEnvironment:
            Ref: MyBatchComputeEnvironment4D8A6583
          Order: 2
      Priority: 1
      State: ENABLED
Rules:
  CheckBootstrapVersion:
    Assertions:
      - Assert:
          Fn::Not:
            - Fn::Contains:
                - - "1"
                  - "2"
                  - "3"
                  - "4"
                  - "5"
                - Ref: BootstrapVersion
        AssertDescription: CDK bootstrap stack version 6 required. Please run 'cdk
          bootstrap' with a recent version of the CDK CLI.
"
`;

